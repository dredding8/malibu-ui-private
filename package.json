{
  "name": "vue-dashboard-blueprint",
  "version": "1.0.0",
  "description": "VUE Dashboard application built with Palantir Blueprint",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:ux": "./run-ux-tests.sh",
    "test:ux:enhanced": "./run-enhanced-ux-tests.sh",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:debug": "playwright test --debug",
    "test:accessibility": "playwright test create-collection-deck-enhanced-ux.spec.ts --grep 'Advanced Accessibility Testing'",
    "test:performance": "jest src/tests/performance/collection-performance.test.tsx --maxWorkers=1",
    "test:visual": "jest src/tests/visual/collection-visual.test.tsx",
    "test:routes": "playwright test --config=src/tests/e2e/playwright.route-validation.config.ts",
    "test:routes:ui": "playwright test --config=src/tests/e2e/playwright.route-validation.config.ts --ui",
    "test:routes:debug": "playwright test --config=src/tests/e2e/playwright.route-validation.config.ts --debug",
    "test:routes:chrome": "playwright test --config=src/tests/e2e/playwright.route-validation.config.ts --project=chromium",
    "test:routes:firefox": "playwright test --config=src/tests/e2e/playwright.route-validation.config.ts --project=firefox",
    "test:routes:webkit": "playwright test --config=src/tests/e2e/playwright.route-validation.config.ts --project=webkit",
    "test:routes:mobile": "playwright test --config=src/tests/e2e/playwright.route-validation.config.ts --project='Mobile Chrome' --project='Mobile Safari'",
    "test:routes:all": "./src/tests/e2e/run-route-validation.sh",
    "test:empathy": "./run-empathy-tests.sh",
    "test:empathy:ui": "./run-empathy-tests.sh -u",
    "test:empathy:debug": "./run-empathy-tests.sh -d",
    "test:empathy:journeys": "playwright test src/tests/e2e/collection-management-user-journeys.spec.ts --config=playwright.empathy.config.ts",
    "test:empathy:scenarios": "playwright test src/tests/e2e/collection-management-empathy-scenarios.spec.ts --config=playwright.empathy.config.ts",
    "test:empathy:report": "playwright show-report test-results/empathy-tests",
    "test:jtbd": "playwright test --config=playwright.jtbd.config.ts",
    "test:jtbd:ui": "playwright test --config=playwright.jtbd.config.ts --ui",
    "test:jtbd:debug": "playwright test --config=playwright.jtbd.config.ts --debug",
    "test:jtbd:headed": "playwright test --config=playwright.jtbd.config.ts --headed",
    "test:jtbd:chrome": "playwright test --config=playwright.jtbd.config.ts --project=desktop-chrome",
    "test:jtbd:all-browsers": "playwright test --config=playwright.jtbd.config.ts",
    "test:jtbd:mobile": "playwright test --config=playwright.jtbd.config.ts --project=mobile-chrome --project=mobile-safari",
    "test:jtbd:a11y": "playwright test --config=playwright.jtbd.config.ts --project=accessibility",
    "test:jtbd:performance": "playwright test --config=playwright.jtbd.config.ts --project=performance",
    "test:jtbd:update-baseline": "JTBD_UPDATE_BASELINE=true playwright test --config=playwright.jtbd.config.ts --project=desktop-chrome",
    "test:jtbd:report": "playwright show-report test-results/jtbd-report",
    "test:jtbd:complete": "./run-jtbd-tests.sh -a",
    "test:jtbd:quick": "./run-jtbd-tests.sh",
    "test:jtbd:watch": "playwright test --config=playwright.jtbd.config.ts --ui --watch",
    "test:visual:capture": "./scripts/manage-visual-baselines.sh capture",
    "test:visual:compare": "./scripts/manage-visual-baselines.sh compare",
    "test:visual:update": "./scripts/manage-visual-baselines.sh update",
    "test:visual:report": "./scripts/manage-visual-baselines.sh report",
    "test:visual:clean": "./scripts/manage-visual-baselines.sh clean",
    "test:visual:ui": "playwright test --config=playwright.visual.config.ts --ui",
    "test:visual:debug": "playwright test --config=playwright.visual.config.ts --debug",
    "test:visual:hub": "playwright test visual-collection-opportunities-hub.spec.ts --config=playwright.visual.config.ts",
    "test:visual:hub:capture": "./scripts/manage-hub-visual-baselines.sh capture",
    "test:visual:hub:compare": "./scripts/manage-hub-visual-baselines.sh compare",
    "test:visual:hub:update": "./scripts/manage-hub-visual-baselines.sh update",
    "test:visual:hub:report": "./scripts/manage-hub-visual-baselines.sh report",
    "test:visual:hub:status": "./scripts/manage-hub-visual-baselines.sh status",
    "test:visual:hub:all": "./scripts/manage-hub-visual-baselines.sh capture -a && ./scripts/manage-hub-visual-baselines.sh compare -a",
    "test:migration": "jest src/components/Collection/__tests__/migration.test.tsx --coverage",
    "test:integration": "jest src/tests/integration/collection-workflows.test.tsx --coverage",
    "test:e2e:collection": "playwright test src/tests/e2e/collection-e2e.test.tsx",
    "test:e2e:collection:headed": "playwright test src/tests/e2e/collection-e2e.test.tsx --headed",
    "test:e2e:collection:debug": "playwright test src/tests/e2e/collection-e2e.test.tsx --debug",
    "test:collection:all": "npm run test:migration && npm run test:integration && npm run test:performance && npm run test:visual && npm run test:e2e:collection",
    "test:collection:unit": "npm run test:migration && npm run test:integration",
    "test:collection:validation": "npm run test:performance && npm run test:visual && npm run test:e2e:collection",
    "test:regression": "playwright test --config=playwright.regression.config.ts",
    "test:regression:watch": "./src/tests/regression/watch-regression.sh",
    "test:regression:quick": "./src/tests/regression/watch-regression.sh --quick",
    "test:regression:ui": "playwright test --config=playwright.regression.config.ts --ui",
    "test:regression:debug": "playwright test --config=playwright.regression.config.ts --debug",
    "test:regression:headed": "playwright test --config=playwright.regression.config.ts --headed",
    "test:regression:report": "playwright show-report test-results/regression-report",
    "test:regression:update": "UPDATE_BASELINES=true playwright test --config=playwright.regression.config.ts",
    "baseline:capture": "ts-node src/tests/regression/baseline-manager.ts capture",
    "baseline:compare": "ts-node src/tests/regression/baseline-manager.ts compare",
    "baseline:update": "ts-node src/tests/regression/baseline-manager.ts update",
    "baseline:report": "ts-node src/tests/regression/baseline-manager.ts report",
    "baseline:rollback": "ts-node src/tests/regression/baseline-manager.ts rollback",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:bundle:html": "node scripts/analyze-bundle.js --html-only",
    "analyze:bundle:json": "node scripts/analyze-bundle.js --json-only",
    "test:coverage:report": "jest --coverage --coverageReporters=html --coverageReporters=json --coverageReporters=text",
    "test:coverage:collection": "jest src/components/Collection src/tests/integration src/tests/performance --coverage --coverageDirectory=test-results/coverage/collection",
    "validate:migration": "npm run test:migration && npm run test:performance && npm run analyze:bundle"
  },
  "dependencies": {
    "@blueprintjs/core": "^6.1.0",
    "@blueprintjs/datetime": "^6.0.1",
    "@blueprintjs/icons": "^6.0.0",
    "@blueprintjs/select": "^6.0.1",
    "@blueprintjs/table": "^6.0.1",
    "@react-aria/interactions": "^3.25.5",
    "@types/react": "^18.3.23",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.7",
    "@types/styled-components": "^5.1.34",
    "chart.js": "^4.5.0",
    "i18next": "^23.2.3",
    "i18next-browser-languagedetector": "^7.2.2",
    "immer": "^10.1.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.3",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.19",
    "typescript": "^4.9.5",
    "web-vitals": "^5.1.0",
    "zustand": "^5.0.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/uuid": "^11.0.0",
    "axe-playwright": "^2.1.0",
    "fs-extra": "^11.3.2",
    "jest-axe": "^10.0.0",
    "pixelmatch": "^6.0.0",
    "pngjs": "^7.0.0"
  }
}
