{
  "name": "vue-dashboard-blueprint",
  "version": "1.0.0",
  "description": "VUE Dashboard application built with Palantir Blueprint",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:ux": "./run-ux-tests.sh",
    "test:ux:enhanced": "./run-enhanced-ux-tests.sh",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:debug": "playwright test --debug",
    "test:accessibility": "playwright test create-collection-deck-enhanced-ux.spec.ts --grep 'Advanced Accessibility Testing'",
    "test:performance": "playwright test create-collection-deck-enhanced-ux.spec.ts --grep 'Performance-Aware UX Testing'",
    "test:visual": "playwright test create-collection-deck-enhanced-ux.spec.ts --grep 'Visual Regression Prevention'"
  },
  "dependencies": {
    "@blueprintjs/core": "^6.1.0",
    "@blueprintjs/datetime": "^6.0.1",
    "@blueprintjs/icons": "^6.0.0",
    "@blueprintjs/select": "^6.0.1",
    "@blueprintjs/table": "^6.0.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/styled-components": "^5.1.34",
    "i18next": "^23.2.3",
    "i18next-browser-languagedetector": "^7.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.3",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.19",
    "typescript": "^4.9.5",
    "web-vitals": "^5.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.54.2",
    "axe-playwright": "^2.1.0"
  }
}
